FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setParticlesDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

boundingBoxMin		(-2.5 -5 0); 	// mimmum coordinates of the fuel bed
boundingBoxMax		(2.5 5 10);	// maximum coordinates of the fuel bed

separationStreamwise	2.5;
separationSpanwise	5;

initState	1;

nParticlesPerSuperParticle   1;

initTimeStep	1e-3;

initSize 	1e-3;

initVelo 	(0 0 0);

initTemp 	300;
initYO2 	0.233;
initGaugeP 	0.0;
initWetSolid	1.0;
initDrySolid	0.0;
initChar	0.0;
initAsh		0.0;

probeLocations
(
	(0.0 0.0 0.0)
);


